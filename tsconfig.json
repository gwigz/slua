{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
	},
	"include": [
		"**/.vitepress/**/*.ts",
		"**/.vitepress/**/*.d.ts",
		"**/.vitepress/**/*.mts",
		"**/.vitepress/**/*.vue"
	],
	"exclude": [
		"node_modules"
	],
	"vueCompilerOptions": {
		"vitePressExtensions": [
			".md"
		],
	},
}